# Database
DB_HOST=mysql
DB_PORT=3306
DB_NAME=misp
DB_USER=misp
DB_PASS=change_me_misp_password

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# MISP base URL (adjust for ACA later)
MISP_BASEURL=http://localhost:8080

# Admin user
MISP_ADMIN_EMAIL=admin@admin.test
MISP_ADMIN_PASSPHRASE=admin

# MISP Configuration
MISP_SALT=change_me_salt_value
MISP_GPG_HOMEDIR=/var/www/MISP/.gnupg
MISP_GPG_PASSWORD=

# Security
MISP_SECURITY_SALT=change_me_security_salt
MISP_SECURITY_CIPHER_SEED=

# Email settings
MISP_EMAIL_REPLY_TO=no-reply@localhost
MISP_EMAIL_FROM=no-reply@localhost

# Worker settings
MISP_WORKERS_WORKER_RESTART_THRESHOLD=10
MISP_WORKERS_WORKER_TIMEOUT=120
